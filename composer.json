{
    "name": "zaimea/heroicons",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "Beautiful hand-crafted SVG icons, by the makers of TailwindLabs, and converted for Blade component.",
    "keywords": [
        "laravel",
        "icons",
        "heroicons",
        "blade",
        "zaimea",
        "zaimealabs",
        "svg"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^10.0",
        "phpstan/phpstan": "^1.12.23",
        "pestphp/pest": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Zaimea\\Heroicons\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "bin": [
        "bin/heroicons"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Zaimea\\Heroicons\\HeroiconsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
