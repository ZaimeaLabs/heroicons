{
    "name": "zaimealabs/heroicons",
    "license": "MIT",
    "homepage": "https://zaimea.com",
    "description": "The ZaimeaLabs Heroicons for Laravel Blade",
    "keywords": [
        "laravel",
        "icons",
        "heroicons",
        "blade",
        "zaimealabs"
    ],
    "authors": [
        {
            "name": "Custură Laurențiu",
            "email": "mail@custura.de"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "ZaimeaLabs\\Heroicons\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "bin": [
        "bin/heroicons"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "ZaimeaLabs\\Heroicons\\HeroiconsServiceProvider"
            ]
        },
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
